msgid "Toolbar Style"
msgstr "Stíll verkfærastiku"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:10
msgid "Toolbar Style. Valid values are \"both\", \"both-horiz\", \"icons\", and \"text\"."
msgstr "Stíll tækjastiku. Leyfð gildi eru \"both\" (bæði), \"both-horiz\" (bæði lárétt), \"icons\" (teikn) og \"text\" (texti)"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:11
msgid "Menus Have Icons"
msgstr "Smátáknmyndir á valmynd"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:12
msgid "Whether menus may display an icon next to a menu entry."
msgstr "Hvort valmyndir megi sýna teikn við hlið valmyndarfærslu."

#: ../schemas/org.gde2.interface.gschema.xml.in.h:13
msgid "Buttons Have Icons"
msgstr "Myndir á tökkum"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:14
msgid "Whether buttons may display an icon in addition to the button text."
msgstr "Hvort takkar hafi myndir og texta."

#: ../schemas/org.gde2.interface.gschema.xml.in.h:15
msgid "Menubar Detachable"
msgstr "Hægt er að flytja valmyndastiku"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:16
msgid "Whether the user can detach menubars and move them around."
msgstr "Hvort notandi geti slitið valmyndastikur frá og fært þær til."

#: ../schemas/org.gde2.interface.gschema.xml.in.h:17
msgid "Toolbar Detachable"
msgstr "Hægt er að flytja tólstiku"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:18
msgid "Whether the user can detach toolbars and move them around."
msgstr "Hvort notandi geti slitið tækjastikur frá og fært þær til."

#: ../schemas/org.gde2.interface.gschema.xml.in.h:19
msgid "Toolbar Icon Size"
msgstr "Stærð smátáknmynda á tólstiku"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:20
msgid "Size of icons in toolbars, either \"small-toolbar\" or \"large-toolbar\"."
msgstr "Stærð teikna í tækjastikum, ýmist \"small-toolbar\" (lítil tækjastika) eða \"large-toolbar\" (stór tækjastika)"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:21
msgid "Cursor Blink"
msgstr "Blikk bendils"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:22
msgid "Whether the cursor should blink."
msgstr "Hvort textabendillinn eigi að blikka."

#: ../schemas/org.gde2.interface.gschema.xml.in.h:23
msgid "Cursor Blink Time"
msgstr "Blikktíðni bendils"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:24
msgid "Length of the cursor blink cycle, in milliseconds."
msgstr "Lengd á blikklotu bendils, í millisekúndum."

#: ../schemas/org.gde2.interface.gschema.xml.in.h:25
msgid "Icon Theme"
msgstr "Táknmyndaþema"

#: ../schemas/org.gde2.interface.gschema.xml.in.h:26
msgid "Icon theme to use for the panel, Caja etc."
msgstr ""

